{
  "name": "react-webpack-starter",
  "version": "0.3.2",
  "description": "React webpack starter template",
  "main": "lib/index.js",
  "author": {
    "name": "Krasimir Tsonev",
    "email": "info@krasimirtsonev.com",
    "url": "http://krasimirtsonev.com"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "carousel",
    "responsive"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:krasimir/react-webpack-starter.git"
  },
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack --config=webpack.build.config.js",
    "test": "better-npm-run start-test",
    "jest-test": "jest",
    "node-jest-test": "node run.jest.test.js",
    "test:ci": "watch 'npm run test' src/",
    "bubel-server": "better-npm-run start-dev",
    "start-server": "better-npm-run start-server"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "babel ./webpack/webpack-dev-server.js --out-file bin/server.js",
      "env": {
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-server": {
      "command": "node ./bin/server.js",
      "env": {
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.1",
    "normalizr": "^3.2.3",
    "path": "^0.12.7",
    "piping": "^1.0.0-rc.4",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "react-transition-group": "^1.1.3",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "superagent": "^3.5.2",
    "webpack": "2.6.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-isomorphic-tools": "^3.0.2"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "better-npm-run": "0.0.15",
    "core-js": "2.4.1",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "jest": "^20.0.3",
    "jest-cli": "^20.0.3",
    "jsx-transform": "^2.3.0",
    "karma": "1.7.0",
    "node-sass": "^4.5.3",
    "phantomjs": "^2.1.7",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "transform-runtime": "0.0.0",
    "watch": "1.0.2"
  }
}
