{
  "name": "react-webpack-starter",
  "version": "0.3.2",
  "description": "React webpack starter template",
  "main": "lib/index.js",
  "author": {
    "name": "Krasimir Tsonev",
    "email": "info@krasimirtsonev.com",
    "url": "http://krasimirtsonev.com"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "carousel",
    "responsive"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:krasimir/react-webpack-starter.git"
  },
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack --config=webpack.build.config.js",
    "test": "better-npm-run start-test",
    "test:ci": "watch 'npm run test' src/",
    "bubel-server": "better-npm-run start-dev",
    "start-server": "better-npm-run start-server"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "babel ./webpack/webpack-dev-server.js --out-file bin/server.js",
      "env": {
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-server": {
      "command": "node ./bin/server.js",
      "env": {
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.22.0",
    "fs": "0.0.1-security",
    "immutable": "^3.8.1",
    "material-ui": "^0.17.0",
    "normalizr": "^3.2.2",
    "path": "^0.12.7",
    "piping": "^1.0.0-rc.4",
    "react": "15.4.2",
    "react-dom": "^15.2.0",
    "react-helmet": "^4.0.0",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "superagent": "^3.5.0",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "2.4.1",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-isomorphic-tools": "^3.0.0"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "^6.22.2",
    "babel-core": "6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "better-npm-run": "0.0.14",
    "chai": "3.5.0",
    "core-js": "2.4.1",
    "css-loader": "^0.26.2",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "jest": "^19.0.2",
    "jsx-transform": "^2.3.0",
    "karma": "1.5.0",
    "karma-chai": "0.1.0",
    "karma-chai-plugins": "0.8.0",
    "karma-chai-sinon": "0.1.5",
    "karma-chrome-launcher": "2.0.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "1.0.5",
    "karma-sinon-chai": "1.2.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "phantomjs": "^2.1.7",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "transform-runtime": "0.0.0",
    "watch": "1.0.2"
  }
}
